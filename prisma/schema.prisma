generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model UrlMap {
  id          String    @id @default(cuid())
  code        String    @unique
  shortenUrl  String    @unique
  targetUrl   String
  clicks      Int       @default(0)
  lastClicked DateTime?
  createdAt   DateTime  @default(now())

  @@map("url_maps")
}